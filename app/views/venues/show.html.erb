<div class="page-header">
  <h3>Venue #<%= @venue.id %></h3>
</div>

<div class="row">
  <div class="col-md-12">
    <dl class="dl-horizontal">
      <dt>Link d</dt>
      <dd><%= @venue.link_d %></dd>

      <dt>Link c</dt>
      <dd><%= @venue.link_c %></dd>

      <dt>Link b</dt>
      <dd><%= @venue.link_b %></dd>

      <dt>Link a</dt>
      <dd><%= @venue.link_a %></dd>

      <dt>Map</dt>
      <dd><%= @venue.map_id %></dd>

      <dt>Long</dt>
      <dd><%= @venue.long %></dd>

      <dt>Lat</dt>
      <dd><%= @venue.lat %></dd>

      <dt>Venue photo</dt>
      <dd><%= @venue.venue_photo %></dd>

      <dt>Venue address</dt>
      <dd><%= @venue.venue_address %></dd>

      <dt>Venue description</dt>
      <dd><%= @venue.venue_description %></dd>

      <dt>Venue name</dt>
      <dd><%= @venue.venue_name %></dd>

    </dl>

    <div class="btn-group btn-group-justified">
      <a href="/venues" class="btn btn-primary">
        Back
      </a>
      <a href="/venues/<%= @venue.id %>/edit" class="btn btn-warning">
        Edit
      </a>
      <a href="/delete_venue/<%= @venue.id %>" class="btn btn-danger" rel="nofollow">
        Delete
      </a>
    </div>
  </div>
</div>
