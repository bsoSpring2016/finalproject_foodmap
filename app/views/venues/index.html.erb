<div class="page-header">
  <h1>
    Venues
    <a href="/venues/new" class="btn btn-lg btn-success">New Venue</a>
  </h1>
</div>

<div class="row">
  <div class="col-md-12">
    <table class="table table-striped table-hover">
      <tr>
        <th>Link d</th>
        <th>Link c</th>
        <th>Link b</th>
        <th>Link a</th>
        <th>Map</th>
        <th>Long</th>
        <th>Lat</th>
        <th>Venue photo</th>
        <th>Venue address</th>
        <th>Venue description</th>
        <th>Venue name</th>
        <th>Actions</th>
      </tr>

      <% @venues.each do |venue| %>
      <tr>
        <td><%= venue.link_d %></td>
        <td><%= venue.link_c %></td>
        <td><%= venue.link_b %></td>
        <td><%= venue.link_a %></td>
        <td><%= venue.map_id %></td>
        <td><%= venue.long %></td>
        <td><%= venue.lat %></td>
        <td><%= venue.venue_photo %></td>
        <td><%= venue.venue_address %></td>
        <td><%= venue.venue_description %></td>
        <td><%= venue.venue_name %></td>
        <td>
          <a href="/venues/<%= venue.id %>" class="btn btn-primary">Show</a>
          <a href="/venues/<%= venue.id %>/edit" class="btn btn-warning">Edit</a>
          <a href="/delete_venue/<%= venue.id %>" class="btn btn-danger" rel="nofollow">Delete</a>
        </td>
      </tr>
      <% end %>
    </table>
  </div>
</div>
